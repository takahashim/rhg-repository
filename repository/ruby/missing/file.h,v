#ifndef _FILE_H_
#define _FILE_H_

#include <fcntl.h>

#define L_SET  0
#define L_CURR 1
#define L_INCR 1
#define L_XTND 2


#define	F_OK		0	/* does file exist */
#define	X_OK		1	/* is it executable by caller */
#define	W_OK		2	/* is it writable by caller */
#define	R_OK		4	/* is it readable by caller */

#endif
