      ac_cv_prog_NM="${ac_tool_prefix}nm"
      break
    fi
  done
  IFS="$ac_save_ifs"
  test -z "$ac_cv_prog_NM" && ac_cv_prog_NM="nm"
fi
fi
NM="$ac_cv_prog_NM"
