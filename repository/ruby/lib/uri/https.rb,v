require 'uri/http'

module URI

=begin

== URI::HTTPS

=== Super Class

((<URI::HTTP>))

=end

  class HTTPS < HTTP
    DEFAULT_PORT = 443
  end
  @@@@schemes['HTTPS'] = HTTPS
end # URI
